{
  "name": "sheep-core",
  "version": "1.0.0",
  "description": "Sheep GraphQL API",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/src/index.js",
    "build": "yarn clean && tsc -p .",
    "dev": "DEBUG=sheep:* ts-node-dev --transpileOnly --no-notify src",
    "clean": "rimraf build",
    "lint": "tslint -p .",
    "test": "yarn build && yarn lint && yarn test:prettier && yarn test:coverage",
    "test:watch": "jest --watch",
    "test:prettier": "prettier --list-different '**/*.{json,md,js,jsx,ts,tsx}'",
    "test:coverage": "jest --coverage",
    "prettier": "prettier --write '**/*.{json,md,js,jsx,ts,tsx}'"
  },
  "contributors": [
    "Magnea Vignisdottir <magneavignisdottir@gmail.co>"
  ],
  "keywords": [
    "sheep"
  ],
  "author": "Magnea Rún Vignisdóttir",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "renovate": {
    "extends": [
      "config:base",
      "schedule:weekly"
    ],
    "automerge": true,
    "major": {
      "automerge": false
    }
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "./__tests__/utils/setup.ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.spec.ts?(x)"
    ]
  },
  "resolutions": {
    "fs-capacitor": "5.0.0"
  },
  "devDependencies": {
    "@accounts/types": "^0.23.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@firebase/app-types": "^0.4.0",
    "@types/debug": "4.1.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/graphql": "^14.2.2",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/graphql-upload": "^8.0.0",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/node": "^13.7.2",
    "@types/swagger-ui-express": "^3.0.1",
    "@types/turndown": "^5.0.0",
    "apollo-server-testing": "^2.6.7",
    "codecov": "^3.8.3",
    "firebase-mock": "^2.2.10",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.1.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-loader": "6.2.1",
    "ts-node-dev": "^1.0.0-pre.40",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "webpack-cli": "3.3.10"
  },
  "dependencies": {
    "@accounts/graphql-api": "^0.23.0",
    "@accounts/password": "^0.23.0",
    "@accounts/server": "^0.23.0",
    "@accounts/typeorm": "^0.23.0",
    "@auto-relay/typeorm": "^0.12.5",
    "@graphql-modules/core": "^0.7.14",
    "@sentry/node": "^5.10.2",
    "@types/graphql-relay": "^0.4.11",
    "@types/graphql-type-json": "^0.3.2",
    "@types/mailgun-js": "^0.22.4",
    "apollo-server": "^2.6.7",
    "apollo-server-express": "^2.7.0",
    "auto-relay": "^0.12.5",
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.2.0",
    "form-data": "^2.5.0",
    "graphql": "^14.4.2",
    "graphql-iso-date": "^3.6.1",
    "graphql-relay": "^0.6.0",
    "graphql-type-json": "^0.3.1",
    "graphql-upload": "9.0.0",
    "handlebars": "^4.1.2",
    "https": "^1.0.0",
    "lodash": "^4.17.13",
    "mailchimp-api-v3": "^1.13.0",
    "mailgun-js": "^0.22.0",
    "moesif-express": "^2.9.15",
    "mysql": "^2.18.1",
    "pg": "^7.18.1",
    "reflect-metadata": "^0.1.13",
    "sofa-api": "^0.3.0",
    "swagger-ui-express": "^4.0.7",
    "turndown": "^5.0.3",
    "type-graphql": "^0.17.4",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.22",
    "typeorm-cursor-connection": "^0.6.0",
    "typeorm-typedi-extensions": "^0.2.3"
  }
}
